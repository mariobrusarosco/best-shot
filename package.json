{
	"name": "best-shot",
	"private": true,
	"version": "0.56.1",
	"scripts": {
		"start": "concurrently \"vite --mode local-dev\" \"cd ../api-best-shot && yarn dev\"",
		"dev": "vite --mode local-dev 2>&1 | tee dev.log",
		"dev-demo": "vite --mode demo",
		"dev-staging": "vite --mode staging",
		"dev-prod": "vite --mode production",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"compile": "tsc --noEmit",
		"test": "vitest run --coverage",
		"test:dev": "vitest",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --write ./src",
		"format": "biome format",
		"format:fix": "biome format --write",
		"check": "biome check && tsc --noEmit",
		"check:fix": "biome check --write && tsc --noEmit",
		"typecheck": "tsc --noEmit",
		"typecheck:src": "tsc --noEmit src/**/*.{ts,tsx}",
		"ci:check": "biome ci",
		"prepare": "husky"
	},
	"dependencies": {
		"@auth0/auth0-react": "^2.2.4",
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@fontsource/montserrat": "^5.1.0",
		"@fontsource/poppins": "^5.1.0",
		"@hookform/resolvers": "^3.3.4",
		"@mui/base": "^5.0.0-beta.70",
		"@mui/material": "latest",
		"@mui/system": "latest",
		"@mui/x-date-pickers": "^8.8.0",
		"@sentry/react": "^8.46.0",
		"@sentry/vite-plugin": "^2.22.7",
		"@tabler/icons-react": "^3.22.0",
		"@tanstack/react-query": "^5.51.9",
		"@tanstack/react-query-devtools": "^5.51.9",
		"@tanstack/react-router": "^1.47.1",
		"@tanstack/react-store": "^0.7.3",
		"@tanstack/store": "^0.7.2",
		"axios": "^1.7.2",
		"dayjs": "^1.11.13",
		"drizzle-zod": "^0.7.0",
		"launchdarkly-react-client-sdk": "^3.6.1",
		"lodash": "^4.17.21",
		"lodash-es": "^4.17.21",
		"motion": "^11.15.0",
		"openai": "^4.89.0",
		"react": "^17.0.0 || ^18.0.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.61.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@mui/mcp": "latest",
		"@playwright/test": "^1.51.1",
		"@tanstack/router-devtools": "^1.47.1",
		"@tanstack/router-plugin": "^1.47.0",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/react": "^15.0.6",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.12.8",
		"@types/react": "^18.3.1",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@vitest/coverage-v8": "^1.5.3",
		"concurrently": "^9.1.0",
		"husky": "^9.1.7",
		"jsdom": "^24.0.0",
		"lint-staged": "^16.1.5",
		"rollup-plugin-visualizer": "^5.12.0",
		"typescript": "^5.4.0",
		"vite": "^5.2.0",
		"vite-plugin-checker": "^0.8.0",
		"vitest": "^1.5.3"
	},
	"peerDependencies": {
		"react": "^17.0.0 || ^18.0.0"
	}
}
